{
	"name": "ghostkit/grid",
	"category": "layout",
    "supports": {
        "html": false,
        "className": false,
        "anchor": true,
        "align": [ "wide", "full" ]
    },
    "attributes": {
        "isTemplatesModalOnly": {
            "type": "boolean"
        },
        "columns": {
            "type": "number",
            "default": 0
        },
        "gap": {
            "type": "string",
            "default": "md"
        },
        "gapCustom": {
            "type": "number"
        },
        "verticalAlign": {
            "type": "string"
        },
        "horizontalAlign": {
            "type": "string"
        }
    }
}
